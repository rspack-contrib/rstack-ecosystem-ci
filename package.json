{
  "name": "rstack-ecosystem-ci",
  "version": "0.1.0",
  "description": "Rstack Ecosystem CI",
  "homepage": "https://github.com/rspack-contrib/rstack-ecosystem-ci#readme",
  "bugs": {
    "url": "https://github.com/rspack-contrib/rstack-ecosystem-ci/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rspack-contrib/rstack-ecosystem-ci.git"
  },
  "license": "MIT",
  "type": "module",
  "scripts": {
    "bisect": "tsx ecosystem-ci.ts bisect",
    "lint": "biome check .",
    "prepare": "pnpm exec simple-git-hooks",
    "test": "tsx ecosystem-ci.ts",
    "website:fetch": "node ./website/scripts/fetch-remote-history.mjs",
    "website:dev": "pnpm run website:fetch && cd website && rsbuild dev",
    "website:dev:mock": "cross-env RSBUILD_PUBLIC_DATA_SOURCE=mock pnpm run website:fetch && cd website && cross-env RSBUILD_PUBLIC_DATA_SOURCE=mock rsbuild dev",
    "website:dev:cached": "cd website && rsbuild dev",
    "website:build": "pnpm run website:fetch && cd website && rsbuild build",
    "website:preview": "cd website && rsbuild preview"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec nano-staged"
  },
  "nano-staged": {
    "*": ["biome check . --write"]
  },
  "dependencies": {
    "@actions/cache": "^4.0.1",
    "@actions/core": "^1.10.0",
    "@fontsource/geist-sans": "^5.0.3",
    "@manypkg/get-packages": "^2.2.2",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@rsbuild/core": "^1.5.17",
    "@rsbuild/plugin-react": "^1.4.1",
    "cac": "^6.7.14",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "execa": "^7.1.1",
    "lucide-react": "^0.356.0",
    "node-fetch": "^3.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.0.2",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@antfu/ni": "^0.20.0",
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^18.15.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/semver": "^7.3.13",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "nano-staged": "^0.8.0",
    "postcss": "^8.4.39",
    "semver": "^7.3.8",
    "simple-git-hooks": "^2.8.1",
    "tailwindcss": "^3.4.6",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=18",
    "pnpm": "10.20.0"
  }
}
